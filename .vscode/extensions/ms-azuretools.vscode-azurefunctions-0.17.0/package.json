		"onCommand:azureFunctions.addBinding",
		"onCommand:azureFunctions.setAzureWebJobsStorage",
		"workspaceContains:host.json",
		"workspaceContains:*/host.json",
		"onView:azureFunctionsExplorer",
		"onDebugInitialConfigurations"
	],
