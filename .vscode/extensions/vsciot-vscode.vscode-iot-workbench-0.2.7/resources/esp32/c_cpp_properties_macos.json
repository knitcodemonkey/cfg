{
    "configurations": [
        {
            "name": "Mac",
            "defines": [
                "ARDUINO=10800"
            ],
            "includePath": [
                "${workspaceFolder}",
                "~/Library/Arduino15/packages/esp32/tools/**",
                "~/Library/Arduino15/packages/esp32/hardware/esp32/{VERSION}/**"
            ],
            "forcedInclude": [
                "~/Library/Arduino15/packages/esp32/hardware/esp32/{VERSION}/cores/esp32/Arduino.h"
            ],
            "intelliSenseMode": "clang-x64",
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}