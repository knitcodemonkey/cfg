{
	"private": true,
	"name": "react-pure-to-class-vscode",
	"displayName": "React Pure To Class",
	"description": "Convert pure react components to class components",
	"version": "1.1.4",
	"publisher": "angryobject",
	"license": "MIT",
	"author": {
		"name": "Max Shishkin"
	},
	"homepage": "https://github.com/angryobject/react-pure-to-class",
	"keywords": [
		"codemod",
		"jscodeshift",
		"react",
		"vscode"
	],
	"main": "./extension.js",
	"engines": {
		"vscode": "^1.12.0"
	},
	"categories": [
		"Formatters",
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.reactPureToClass"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.reactPureToClass",
				"title": "React Pure To Class"
			}
		],
		"configuration": {
			"type": "object",
			"title": "React Pure To Class",
			"properties": {
				"reactPureToClass.typescript": {
					"type": "boolean",
					"default": false,
					"description": "Support TypeScript files. Experimental, may or may not work"
				}
			}
		}
	},
	"scripts": {
		"watch": "webpack --watch",
		"build": "webpack"
	},
	"devDependencies": {
		"jscodeshift": "^0.4.0",
		"react-pure-to-class": "^1.1.1",
		"vscode": "^1.1.10",
		"webpack": "^3.10.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/angryobject/react-pure-to-class"
	},
	"icon": "icon.png",
	"__metadata": {
		"id": "b5bd2855-8c9a-4faf-b6ae-18f163b1418f",
		"publisherId": "451fa213-8cbd-4045-9972-37375233a9b2",
		"publisherDisplayName": "Max Shishkin"
	}
}